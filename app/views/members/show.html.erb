<% provide(:title, @member.username) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= image_tag "members/team-member.gif", width: '50', height: '50' %>
        <%= @member.username %>
      </h1>
    </section>
  </aside>
  <!--display timeline post-->
  <div class="col-md-8">
    <% if @member.timelines.any? %>
      <h3>Timelines (<%= @member.timelines.count %>)</h3>
      <ol class="timelines">
        <%= render @timelines %>
      </ol>
      <%= will_paginate @timelines %>
    <% end %>
  </div>
</div>